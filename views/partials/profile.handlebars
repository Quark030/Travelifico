<script src="/utils/profile.js"></script> <!-- Include your JavaScript file for profile functionality -->

<header class="bg-blue-600 text-white shadow-lg p-6">
    <h1 class="text-3xl font-semibold">User Profile</h1>
</header>
<main class="bg-blue-100 p-6">
    <section class="profile-info bg-white shadow-lg rounded-lg p-6">
        <h2 class="text-2xl font-semibold text-blue-700 mb-4">User Information</h2>
        <ul>
            <li class="mb-2"><strong class="text-blue-700">Email:</strong> <span id="user-email" class="ml-2">{{userData.email}}</span></li>
            <li><strong class="text-blue-700">Phone Number:</strong> <span id="user-phone" class="ml-2">{{userData.phone}}</span></li>
            <!-- Add more user information fields as needed -->
        </ul>
    </section>
</main>


    